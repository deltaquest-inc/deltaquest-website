{
  "name": "deltaquest-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "build": "turbo run build",
    "dev": "node scripts/dev.js",
    "dev:it": "turbo run dev --filter=@deltaquest/it",
    "dev:lasertag": "turbo run dev --filter=@deltaquest/lasertag",
    "dev:games": "turbo run dev --filter=@deltaquest/games",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "install:all": "yarn install && yarn workspaces foreach run install"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^9.14.0",
    "prettier": "3.6.2",
    "turbo": "^1.10.12",
    "typescript": "^5.1.3"
  }
}
